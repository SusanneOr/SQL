/** Aggregate the total number of transaction per country to determine which country produces the highest revenue**/

SELECT COUNT(ID), SUM(price), country
FROM sales
GROUP BY country
ORDER BY SUM(PRICE) desc;

/**Which payment type is most commoun outside of United States**/
SELECT SUM(price),payment_type
FROM sales
WHERE country != "United states"
GROUP BY payment_type
ORDER BY payment_type desc;


/*Which citys are on the top 10 list with most sales*/

SELECT city, SUM(price)
FROM sales
GROUP BY city
ORDER BY SUM(price) desc
LIMIT 10
;
